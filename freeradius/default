server default {
    listen {
        type = auth
        ipaddr = *
        port = 1812
        limit {
            max_connections = 16
            lifetime = 0
            idle_timeout = 30
        }
    }

    listen {
        type = acct
        ipaddr = *
        port = 1813
    }

    listen {
        type = auth
        ipv6addr = ::
        port = 1812
        limit {
            max_connections = 16
            lifetime = 0
            idle_timeout = 30
        }
    }

    listen {
        type = acct
        ipv6addr = ::
        port = 1813
    }

    authorize {
        filter_username
        preprocess
        chap
        mschap
        digest
        suffix
        ldap
        if (notfound) {
            update control {
                Proxy-To-Realm := 'tpeduaaa'
            }
        }
        expiration
        logintime
        pap
    }

    authenticate {
        Auth-Type PAP {
            pap
        }
        Auth-Type CHAP {
            chap
        }
        Auth-Type MS-CHAP {
            mschap
        }
        Auth-Type LDAP {
            ldap
        }
        digest
    }

    preacct {
        preprocess
        acct_unique
        suffix
    }

    accounting {
        detail
        unix
        exec
        attr_filter.accounting_response
    }

    session {
    }

    post-auth {
        update {
            &reply: += &session-state:
        }
        exec
        remove_reply_message_if_eap
        Post-Auth-Type REJECT {
            attr_filter.access_reject
            remove_reply_message_if_eap
        }
    }

    pre-proxy {
    }

    post-proxy {
    }
}