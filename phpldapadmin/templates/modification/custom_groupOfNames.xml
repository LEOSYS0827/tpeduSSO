<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "template.dtd">

<template>
<askcontainer>1</askcontainer>
<description>新增靜態群組</description>
<icon>ldap-uniquegroup.png</icon>
<invalid>0</invalid>
<rdn>cn</rdn>
<regexp>^cn=.*,ou=group,dc=tp,dc=edu,dc=tw</regexp>
<title>群組：靜態(巢狀)</title>
<visible>1</visible>

<objectClasses>
<objectClass id="groupOfNames"></objectClass>
</objectClasses>

<attributes>
<attribute id="cn">
	<display>群組英文縮寫</display>
	<order>1</order>
	<page>1</page>
</attribute>
<attribute id="o">
	<display>作用範圍</display>
	<type>select</type>
	<value><![CDATA[=php.PickList(/;(&(objectClass=tpeduSchool));dc;%description%)]]></value>
	<order>2</order>
	<page>1</page>
</attribute>
<attribute id="member">
	<display>群組成員</display>
	<order>3</order>
	<page>1</page>
</attribute>
<attribute id="description">
	<display>用途說明</display>
	<order>4</order>
	<page>1</page>
</attribute>
<attribute id="owner">
	<display>管理者</display>
	<hint>請輸入管理員的完整 DN。</hint>
	<order>5</order>
	<page>1</page>
</attribute>
</attributes>

</template>
